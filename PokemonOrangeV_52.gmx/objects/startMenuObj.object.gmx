            <string>///Fading
if !(self.allFade) {
    if !(self.fadeOne = 1) {
        self.fadeOne += .01
    }
    if !(self.beginFadeOut) {
        self.fadeTwo += .05
        if self.fadeTwo = 1 {
            self.beginFadeOut = true
        }
    }
    else if (self.beginFadeOut) {
        self.fadeTwo -= .05
        if self.fadeTwo = 0 {
            self.beginFadeOut = false
        }
    }
}

//enter pressed
if (self.allFade) {
    if !(self.fadeOne &lt;= 0) {
        self.fadeOne -= .05
    }
    if !(self.fadeTwo &lt;= 0) {
        self.fadeTwo -= .05
    }
    if (self.fadeTwo &lt;= 0) and (self.fadeOne &lt;= 0) {
        instance_create(self.x, self.y, mainMenuObj)
        instance_destroy()
    }
}
